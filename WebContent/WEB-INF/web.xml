<?xml version="1.0" encoding="UTF-8"?>
<web-app 
  xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
  version="3.0">

	<servlet>
		<servlet-name>CreationClient</servlet-name>
		<servlet-class>com.trung.servlets.CreationClient</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>CreationClient</servlet-name>
		<url-pattern>/createClient</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>CreationCommand</servlet-name>
		<servlet-class>com.trung.servlets.CreationCommand</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>CreationCommand</servlet-name>
		<url-pattern>/createCommand</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ClientInformation</servlet-name>
		<servlet-class>com.trung.servlets.ClientInformation</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>ClientInformation</servlet-name>
		<url-pattern>/clientInformation</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>CommandInformation</servlet-name>
		<servlet-class>com.trung.servlets.CommandInformation</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>CommandInformation</servlet-name>
		<url-pattern>/commandInformation</url-pattern>
	</servlet-mapping>
</web-app>